{
    "plugins": [
        "prettier"
    ],
    "extends": [
        "react-app",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "printWidth": 200,
                "semi": true,
                "jsxSingleQuote": true,
                "singleQuote": true,
                "useTabs": true,
                "arrowParens": "avoid"
            }
        ]
    }
}